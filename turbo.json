{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [
    ".env"
  ],
  "pipeline": {
    "start": {
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**",
        "!.next/cache/**"
      ]
    },
    "check": {},
    "@zeswen/product-service#check": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "migrate": {},
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "test:dev": {
      "cache": false,
      "persistent": true
    }
  }
}