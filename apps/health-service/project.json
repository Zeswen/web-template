{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@zeswen/health-service",
  "root": "apps/health-service",
  "sourceRoot": "apps/health-service/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build --release",
        "cwd": "apps/health-service"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run",
        "cwd": "apps/health-service"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go lint",
        "cwd": "apps/health-service"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run --release",
        "cwd": "apps/health-service"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test",
        "cwd": "apps/health-service"
      }
    }
  }
}